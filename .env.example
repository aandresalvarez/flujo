# Example environment configuration for flujo
# Copy this file to `.env` and replace placeholders with your real keys.

OPENAI_API_KEY=YOUR_KEY_HERE
GOOGLE_API_KEY=
ANTHROPIC_API_KEY=
# LOGFIRE_API_KEY=YOUR_LOGFIRE_KEY

# --- optional toggles ---
REFLECTION_ENABLED=true
REWARD_ENABLED=false
# MAX_ITERS=3
# K_VARIANTS=2
AGENT_TIMEOUT=60
TELEMETRY_EXPORT_ENABLED=false
# OTLP_EXPORT_ENABLED=true
# OTLP_ENDPOINT=https://otlp.example.com

# Default models for orchestrator agents
DEFAULT_SOLUTION_MODEL=openai:gpt-4o
DEFAULT_REVIEW_MODEL=openai:gpt-4o
DEFAULT_VALIDATOR_MODEL=openai:gpt-4o
DEFAULT_REFLECTION_MODEL=openai:gpt-4o
