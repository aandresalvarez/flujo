# Example environment configuration for pydantic-ai-orchestrator
# Copy this file to `.env` and replace placeholders with your real keys.

ORCH_OPENAI_API_KEY=YOUR_KEY_HERE
# ORCH_LOGFIRE_API_KEY=YOUR_LOGFIRE_KEY

# --- optional toggles ---
ORCH_REFLECTION_ENABLED=true
ORCH_REWARD_ENABLED=false
# ORCH_MAX_ITERS=3
# ORCH_K_VARIANTS=2
ORCH_AGENT_TIMEOUT=60
ORCH_TELEMETRY_EXPORT_ENABLED=false
# ORCH_OTLP_EXPORT_ENABLED=true
# ORCH_OTLP_ENDPOINT=https://otlp.example.com
